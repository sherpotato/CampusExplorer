{
    "title": "Transformation: applykey and complex order",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "cpsc"
                    }
                },
                {
                    "IS": {
                        "courses_id": "304"
                    }
                },
                {
                    "NOT": {
                        "IS": {
                            "courses_instructor": ""
                        }
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_instructor",
                "overallAvg"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "overallAvg"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept",
                "courses_id",
                "courses_instructor"
            ],
            "APPLY": [
                {
                    "overallAvg": {
                        "AVG": "courses_avg"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
